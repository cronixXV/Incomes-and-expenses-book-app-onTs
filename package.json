  "scripts": {
    "prettier": "prettier --write ./src/**/*.js",
    "prettier:check": "prettier --check ./src/**/*.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint --fix . --ext .js",
    "build": "webpack --mode production --node-env=production",
    "dev": "webpack --mode development --node-env=development",
    "watch": "webpack --watch --mode development --node-env=development",
    "serve:webpack": "webpack serve --mode development --node-env=development",
    "serve:production": "serve -s dist",
    "serve:json": "json-server --watch db.json --port 3001 --delay 1000",
    "start": "concurrently \"npm:serve:webpack\" \"npm:serve:json\"",
    "start:production": "concurrently \"npm:serve:production\" \"npm:serve:json\"",
    "test": "echo \"Error: no test specified\" && exit 1"
